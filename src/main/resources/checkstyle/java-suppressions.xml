<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suppressions PUBLIC "-//Puppy Crawl//DTD Suppressions 1.1//EN" "http://www.puppycrawl.com/dtds/suppressions_1_1.dtd">
<suppressions>

	<suppress id="ImportControl" files="RequestDefaultsConfig\.java" />
	<suppress checks="ImportControl" files="[a-zA-Z0-9]*Test\.java" />
	<suppress checks="MemberName|MethodName|ParameterName|RegexpSinglelineJava" files="(Scripts)\.java" />

	<!-- Logs and its test class are exempt from logging Checkstyle -->
	<suppress id="log4jDebug" files="(Logs|LogsTest)\.java" />
	<suppress id="log4jInfo" files="(Logs|LogsTest)\.java" />
	<suppress id="log4jWarn" files="(Logs|LogsTest)\.java" />
	<suppress id="log4jError" files="(Logs|LogsTest)\.java" />

</suppressions>
